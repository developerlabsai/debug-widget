{
  "name": "@developerlabs/debug-widget",
  "version": "1.0.0",
  "description": "Embeddable browser widget for capturing debug data and interactive Q&A with Claude Code",
  "type": "module",
  "main": "dist/debug-widget.umd.js",
  "module": "dist/debug-widget.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/debug-widget.es.js",
      "require": "./dist/debug-widget.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "debug",
    "widget",
    "claude",
    "development-tools",
    "browser"
  ],
  "author": "Developer Labs AI",
  "license": "MIT",
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html-to-image": "^1.11.13",
    "preact": "^10.19.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20.11.0",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
